<html>
<head>
	<script type="text/javascript">
		function check_it(){
			var tomatch=/[A-Za-z0-9]\w{2,}\@[A-Za-z0-9]{3,}\.[A-Za-z]{3}/;
			var emadd=document.f1.t1.value;
			if(tomatch.test(emadd)){
				alert("E-mail address OK");
				return true;
			}
			else{
				alert("E-mail address invalid. Please Try again");
				return false;
			}
		}
	</script>
</head>
<body>
	<form name="f1" id="f1" action="1.html" onsubmit="return check_it()">
		Enter your email address:<br><input type="text" name="t1" id="t1"><br><input type="submit">
	</form>
</body>
</html>