	<title> </title>
</head>
<body>
<script type="text/javascript">
function add(n1,n2){
alert(n1+n2);
}
	function sub(n1,n2){
if (n1>n2) {
	alert(n1-n2);
}
else
alert(n2-n1);
}
function mul(n1,n2){
alert(n1*n2);
}
function div(n1,n2){
alert(n1/n2);
}
</script>
<input type="button" value="+" onclick="add(10,20)">
	<input type="button" value="-" onclick="sub(10,20)">
	<input type="button" value="*" onclick="mul(10,20)">
	<input type="button" value="/" onclick="div(10,20)">
</body>
</html>